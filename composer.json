    "aws/aws-sdk-php": "~3.0",
    "doctrine/collections": "~1.0",
    "zendframework/zend-hydrator": "~2.0",
    "zendframework/zend-filter": "~2.6",
    "zendframework/zend-servicemanager": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mikey179/vfsStream": "~1.6",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
